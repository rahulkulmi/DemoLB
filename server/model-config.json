{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongoDB"
  },
  "AccessToken": {
    "dataSource": "mongoDB",
    "public": false
  },
  "ACL": {
    "dataSource": "mongoDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDB",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongoDB",
    "public": false
  },
  "Provider": {
    "dataSource": "mongoDB",
    "public": true
  },
  "Job": {
    "dataSource": "mongoDB",
    "public": true
  },
  "Customer": {
    "dataSource": "mongoDB",
    "public": true
  },
  "Address": {
    "dataSource": "mongoDB",
    "public": true
  },
  "Feed": {
    "dataSource": "mongoDB",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getFeeds": true,
          "createFeeds": true
        }
      }
    }
  },
  "XYZ": {
    "dataSource": null,
    "public": true
  },
  "push": {
    "dataSource": "push",
    "public": true
  },
  "application": {
    "dataSource": "mongoDB",
    "public": true
  },
  "installation": {
    "dataSource": "mongoDB",
    "public": true
  },
  "notification": {
    "dataSource": "mongoDB",
    "public": true
  }
}
